{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/server",
    "dev": "ts-node-dev --respawn --transpile-only ./server",
    "build": "test -e .env || cp .env.example .env && tsc && cp .env ./dist",
    "lint": "eslint . --max-warnings 0",
    "test-build": "tsc && cp .env.test ./dist/.env",
    "test-start": "npm run test-build && cd dist && NODE_ENV=test node ./server",
    "test": "jest --no-watchman --detectOpenHandles",
    "test:watch": "jest --no-watchman --detectOpenHandles --watch",
    "coverage": "jest --collect-coverage --detectOpenHandles --no-watchman",
    "generate:key": "bash cmd/generateKeys.sh"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^6.3.0",
    "argon2": "^0.31.2",
    "body-parser": "^1.20.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^8.0.3",
    "nanoid": "3.3.4",
    "node-file-router": "^0.4.0",
    "nodemailer": "^6.9.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-themes": "^1.2.30",
    "swagger-ui-express": "^5.0.0",
    "typescript": "^5.2.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-mongodb": "^5.1.1",
    "winston-transport": "^4.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@strategist-cms/eslint-config": "workspace:*",
    "@strategist-cms/typescript-config": "workspace:*",
    "@typegoose/typegoose": "^12.0.0",
    "@types/argon2": "^0.15.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.1",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  }
}
